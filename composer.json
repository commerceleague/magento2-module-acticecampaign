{
  "name": "commerceleague/magento2-module-activecampaign",
  "description": "magento2 active campaign connector",
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true,
      "magento/composer-dependency-version-audit-plugin": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true
    }
  },
  "require": {
    "php": "^8.2",
    "commerceleague/api-client-activecampaign": "dev-version-2",
    "http-interop/http-factory-guzzle": "^1.0",
    "magento/framework": "103.0.*",
    "magento/framework-message-queue": "100.4.*",
    "magento/module-customer": "103.0.*",
    "php-http/guzzle7-adapter": "^1.0"
  },
  "type": "magento2-module",
  "license": "MIT",
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "CommerceLeague\\ActiveCampaign\\": ""
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  ],
  "require-dev": {
    "bitexpert/phpstan-magento": "^0.11.0",
    "magento/magento-coding-standard": "^33",
    "phpstan/extension-installer": "^1.3",
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^11.0.3",
    "rector/rector": "^0.17.13"
  }
}
