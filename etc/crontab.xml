<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
	<group id="default">
		<job name="activecampaign_publish_omitted_contacts" instance="CommerceLeague\ActiveCampaign\Cron\PublishOmittedContacts" method="run">
			<schedule>*/10 * * * *</schedule>
		</job>
		<job name="activecampaign_publish_omitted_customers" instance="CommerceLeague\ActiveCampaign\Cron\PublishOmittedCustomers" method="run">
			<schedule>*/10 * * * *</schedule>
		</job>
		<job name="activecampaign_publish_omitted_orders" instance="CommerceLeague\ActiveCampaign\Cron\PublishOmittedOrders" method="run">
			<schedule>*/10 * * * *</schedule>
		</job>
		<job name="activecampaign_publish_omitted_abandoned_carts" instance="CommerceLeague\ActiveCampaign\Cron\PublishOmittedAbandonedCarts" method="run">
			<schedule>*/10 * * * *</schedule>
		</job>

		<job name="activecampaign_publish_omitted_guest_customers" instance="CommerceLeague\ActiveCampaign\Cron\PublishOmittedGuestCustomers" method="run">
			<schedule>*/10 * * * *</schedule>
		</job>
	</group>
</config>
